using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.SceneManagement;

public class Usedisciption : MonoBehaviour
{
    // 버튼을 눌렀을 때 호출되는 함수
    public void LoadUseDescription()
    {
        // "사막" 스테이지로 이동
        SceneManager.LoadScene("사용설명");
    }
}
