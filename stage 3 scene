using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.SceneManagement;

public class nextstage2 : MonoBehaviour
{
    // 버튼을 눌렀을 때 호출되는 함수
    public void LoadSecondStage()
    {
        // "사막" 스테이지로 이동
        Time.timeScale = 1f;
        SceneManager.LoadScene("Stage3");
    }
}
